                                                int codigoConvocatoriaActual = 0;
                                                int codigoTipoDocumentoActual = 0;
                                            }
                                            @{
                                                var documentosFiltrados = Model.DocumentoConvocatoriasAgrupados
                                                .Where(grupo => grupo.CodigoTipoConvocatoriaId == @Enumerado.TipoConvocatoriaId.Cas)
                                                .ToList();
                                            }
                                            @foreach (var grupo in documentosFiltrados)
                                            {
                                                <div class="row gy-4">
                                                    <div class="col-lg-12">
                                                        @if (codigoConvocatoriaActual != grupo.CodigoConvocatoriaId)
