                                                                        </div>
                                                                        <div class="col-6">
                                                                            <p><strong>@Html.Raw(GeneralResource.EtiquetaFechaFinal)</strong> @grupo.FechaFinal.ToString("yyyy-MM-dd")</p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
