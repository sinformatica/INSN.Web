                                                                <div class="block-content block-content-full text-center">
                                                                    <div class="row g-sm mt-3">
                                                                        <div class="col-6">
                                                                            <p><strong>@Html.Raw(GeneralResource.EtiquetaFechaInicio)</strong> @grupo.FechaInicio.ToString("yyyy-MM-dd")</p>
                                                                        </div>
                                                                        <div class="col-6">
                                                                            <p><strong>@Html.Raw(GeneralResource.EtiquetaFechaFinal)</strong> @grupo.FechaFinal.ToString("yyyy-MM-dd")</p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            codigoConvocatoriaActualReasignación = grupo.CodigoConvocatoriaId;
                                                            codigoTipoDocumentoActualReasignación = 0;
                                                        }
                                                    </div>
                                                    <div class="col-lg-12">
                                                        <table class="table table-borderless table-vcenter mb-4">
                                                            @foreach (var detalle in grupo.Detalles)
                                                            {
                                                                <tbody>
                                                                    @if (codigoTipoDocumentoActualReasignación != detalle.CodigoTipoDocumentoConvocatoriaId)
                                                                    {
                                                                        <tr class="table-active">
                                                                            <th style="width: 50px;"></th>
                                                                            <th>@detalle.DescripcionTipoDocumentoConvocatoria</th>
                                                                        </tr>
                                                                        codigoTipoDocumentoActualReasignación = detalle.CodigoTipoDocumentoConvocatoriaId;
                                                                    }
                                                                    <tr>
                                                                        <td class="table-success text-center">
                                                                            <i class="fa fa-file-lines text-success"></i>
                                                                        </td>
                                                                        <td>
                                                                            <a>@detalle.DescripcionDocumentoConvocatoria</a>
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            }
                                                        </table>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </section>
                                </div>
                            </div>
                        </div>
                        <!-- ======= FIN NOMBRAMIENTO ======= -->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ======= FIN CAS - INTERNO - PUBLICO - ETC ======= -->
    <!-- ======= FIN DE SESSION CON PESTAÑA ======= -->
</main>