<script>
    document.addEventListener("DOMContentLoaded", function () {
        var menus = document.querySelectorAll(".menu");

        menus.forEach(function (menu) {
            var items = menu.querySelectorAll(".menu-item");

            items.forEach(function (item) {
                var link = item.querySelector("a[href]");
                var subMenu = item.querySelector(".sub-menu");

                if (link && subMenu) {
                    // Movemos el evento click a otro elemento dentro del ítem del menú
                    var linkToClick = item.querySelector('.dcjq-parent');
                    linkToClick.addEventListener("click", function (event) {
                        event.preventDefault(); // Evitar la acción predeterminada del enlace

                        subMenu.style.display = subMenu.style.display === "none" ? "block" : "none";

                        // Cerrar otros submenús desplegados en este menú
                        items.forEach(function (otherItem) {
                            if (otherItem !== item && otherItem.querySelector(".sub-menu")) {
                                otherItem.querySelector(".sub-menu").style.display = "none";
                            }
                        });
                    });

                    // Manejamos el clic en el enlace directo para que abra la URL
                    link.addEventListener("click", function (event) {
                        event.stopPropagation(); // Detener la propagación para no cerrar el submenú al hacer clic en el enlace
                    });
                }
            });
        });
    });
</script>