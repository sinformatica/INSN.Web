<body>
    <!-- ======= Header ======= -->
    <section id="topbar" class="topbar d-flex align-items-center">
        <div class="container d-flex justify-content-center justify-content-md-between">
            <div class="contact-info d-flex align-items-center">
                <button onclick="window.open('https://www.transparencia.gob.pe/enlaces/pte_transparencia_enlaces.aspx?id_entidad=13180&id_tema=1&ver=D#.ZEavMnZBwdU/', '_blank');" type="button" class="btn btn-outline-primary btn-sm me-1 mb-1">
                    <img src="~/imagenes/iconos/portal-transparencia-estandar.jpeg" width="42" height="35" />
                    Portal de Transparencia
                </button>
            </div>
            <div class="social-links d-none d-md-flex align-items-center">
                <a href="https://mail.insn.gob.pe/" class="envelope" target="_blank"><i class="bi bi-envelope-fill"></i></a>
                <a href="https://www.youtube.com/INSNvideos" class="youtube" target="_blank"><i class="bi bi-youtube"></i></a>
               @*  <a href="https://twitter.com/insn_oficial" class="twitter" target="_blank"><i class="bi bi-twitter"></i></a> *@
                <a href="https://twitter.com/insn_oficial" class="twitter" target="_blank"><i class="fab fa-twitter"></i></a>
                <a href="https://www.facebook.com/INSNoficial/" class="facebook" target="_blank"><i class="bi bi-facebook"></i></a>
                <a href="https://www.instagram.com/hospital_del_nino_insn/?utm_source=ig_web_button_share_sheet&igshid=OGQ5ZDc2ODk2ZA==" class="instagram" target="_blank"><i class="bi bi-instagram"></i></a>
                <a href="https://www.linkedin.com/company/insn/about/" class="linkedin" target="_blank"><i class="bi bi-linkedin"></i></a>

                @if (HttpContextAccessor?.HttpContext?.Session.GetString(Constantes.JwtToken) != null
                && HttpContextAccessor?.HttpContext?.Session.GetString(Constantes.JwtToken)!.Length > 0)
                {
                    <div style="padding-left: 10px;">
                        <span>Hola @User.Identity?.Name</span>
                        <a class="nav-link text-dark" asp-controller="Acceso" asp-action="Logout">Salir</a>
                    </div>
                }
                else
                {
                    <a asp-controller="Acceso" asp-action="Login" class="linkedin" target="_blank"><i class="bi bi-person-fill"></i></a>
                }
            </div>
        </div>
    </section><!-- End Top Bar -->

    <header id="header" class="header d-flex align-items-center">
