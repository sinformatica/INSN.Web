<script>
    function showTabEspecilidad() {
      
        // Oculta el contenido de Especialidad
        var TabEspecialidad = document.getElementById('TabContentEspecialidad');
        var ButtonEspecialidad = document.getElementById('Button-Especialidad-tab');
        var TabServicio = document.getElementById('TabContentServicio');
        var ButtonServicio = document.getElementById('Button-Servicio-tab');


        TabEspecialidad.style.display = 'block';
        TabServicio.style.display = 'none';


        ButtonEspecialidad.classList.add('active');
        ButtonServicio.classList.remove('active');
    }



    function showTabServicio() {
        var TabEspecialidad = document.getElementById('TabContentEspecialidad');
        var ButtonEspecialidad = document.getElementById('Button-Especialidad-tab');
        var TabServicio = document.getElementById('TabContentServicio');
        var ButtonServicio = document.getElementById('Button-Servicio-tab');


        TabServicio.style.display = 'block';
        TabEspecialidad.style.display = 'none';


        ButtonEspecialidad.classList.remove('active');
        ButtonServicio.classList.add('active');
    }
</script>